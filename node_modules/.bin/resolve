#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules/resolve/bin/node_modules:/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules/resolve/node_modules:/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules:/home/jonathan/Documents/software_development/dicoding/node_modules:/home/jonathan/Documents/software_development/node_modules:/home/jonathan/Documents/node_modules:/home/jonathan/node_modules:/home/node_modules:/node_modules:/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules/resolve/bin/node_modules:/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules/resolve/node_modules:/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules:/home/jonathan/Documents/software_development/dicoding/node_modules:/home/jonathan/Documents/software_development/node_modules:/home/jonathan/Documents/node_modules:/home/jonathan/node_modules:/home/node_modules:/node_modules:/home/jonathan/Documents/software_development/dicoding/notes-app-back-end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../resolve/bin/resolve" "$@"
fi
